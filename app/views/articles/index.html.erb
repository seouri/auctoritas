<h1>Articles</h1>

<table>
  <tr>
    <th>Journal</th>
    <th>Year</th>
    <th>Volume</th>
    <th>Issue</th>
    <th>Pagination</th>
    <th>Title</th>
  </tr>

<% for article in @articles %>
  <tr>
    <td><%=h article.journal %></td>
    <td><%=h article.year %></td>
    <td><%=h article.volume %></td>
    <td><%=h article.issue %></td>
    <td><%=h article.pagination %></td>
    <td><%= link_to article.title, article %></td>
  </tr>
<% end %>
</table>


<div class="pagination">
<%= will_paginate @organizations %>
</div>
